        "/api/auth/sendCode": {
            "post": {
                "tags": [
                    "Auth"
                ],
                "summary": "Send OTP Code",
                "description": "Send OTP code to user's phone number",
                "operationId": "e09c538c074a7b4a0f0c055ec2893f49",
                "requestBody": {
                    "description": "User phone number",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "properties": {
                                    "phone": {
                                        "type": "string",
                                        "example": "09123456789"
                                    },
                                    "_token": {
                                        "description": "CSRF Token",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "لطفا رمز خود را وارد کنید"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "اطلاعات ارسال شده درست نبوده است"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "csrf": []
                    }
                ]
            }
        },
